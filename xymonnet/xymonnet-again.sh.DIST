#!/bin/sh

# This extension script picks up the $XYMONTMP/frequenttests.$XYMONNETWORK
# file generated by xyxmonnet. It then re-does the network tests
# with the same parameters.

REDOFILE=$XYMONTMP/frequenttests.$XYMONNETWORK
if test -r $REDOFILE
then
	@RUNTIMEDEFS@ $XYMONHOME/bin/xymonnet `cat $REDOFILE`
fi

exit 0

